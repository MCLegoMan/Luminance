{
  "schemaVersion": 1,
  "id": "luminance",
  "version": "${version}",
  "name": "Luminance",
  "description": "Luminance is a library mod that is primarily designed to help other mods with shaders.",
  "authors": [
    "MCLegoMan",
    "Nettakrim"
  ],
  "contributors": [
  ],
  "contact": {
    "homepage": "https://mclegoman.com/Luminance",
    "sources": "https://github.com/MCLegoMan/Luminance",
    "issues": "https://github.com/MCLegoMan/Luminance/issues"
  },
  "license": [
    "LGPL-3.0-or-later"
  ],
  "icon": "assets/luminance/icons/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.mclegoman.luminance.client.LuminanceClient"
    ],
    "modmenu": [
	    "com.mclegoman.luminance.compat.ModMenuCompat"
    ]
  },
  "mixins": [
    "mclegoman-luminance.mixins.json"
  ],
  "depends": {
    "minecraft": [
      "~1.21.2-alpha"
    ],
    "fabric-resource-loader-v0": "*",
    "fabric-key-binding-api-v1": "*",
    "fabric-lifecycle-events-v1": "*",
    "java": ">=21"
  },
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ],
      "links": {
        "mclegoman.donate": "https://buymeacoffee.com/mclegoman"
      }
    },
    "catalogue": {
      "configFactory": "com.mclegoman.luminance.compat.CatalogueCompat"
    }
  }
}